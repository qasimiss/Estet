                        img(class="b-oval-green" src="img/icons/oval-green.svg", alt="")
                    picture.b-picture.b-picture--techn
                        source(type="image/webp" srcset="img/content/mobile/7-technology/3/1-1x.webp 1x, ../img/content/mobile/7-technology/3/1-2x.webp 2x" media="(max-width: 767px)") 
                        source(type="image/jpeg" srcset="img/content/mobile/7-technology/3/1-1x.jpg 1x, ../img/content/mobile/7-technology/3/1-2x.jpg 2x" media="(max-width: 767px)")
                        source(type="image/webp" srcset="img/content/desktop/7-technology/3/1-1x.webp 1x, ../img/content/desktop/7-technology/3/1-2x.webp 2x") 
                        source(type="image/jpeg" srcset="img/content/desktop/7-technology/3/1-1x.jpg 1x, ../img/content/desktop/7-technology/3/1-2x.jpg 2x")
                        img(src="../img/content/desktop/7-technology/3/1-1x.jpg")
                    .b-technology__post.b-technology__post--company
