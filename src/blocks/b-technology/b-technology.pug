                    a(class="b-technology__link-hover b-technology__link-hover--lamp" href="javascript:void(0);")
                        img(class="b-oval-white" src="img/icons/oval-white.svg", alt="")
                    picture.b-picture.b-picture--lamp
                        source(type="image/jpeg" srcset="img/content/mobile/7-technology/4/1-1x.jpg 1x, img/content/mobile/7-technology/4/1-2x.jpg 2x" media="(max-width: 767px)")
                        source(type="image/webp" srcset="img/content/tablet/7-technology/4/1-1x.webp 1x, img/content/tablet/7-technology/4/1-2x.webp 2x" media="(max-width: 1023px)") 
                        source(type="image/jpeg" srcset="img/content/tablet/7-technology/4/1-1x.jpg 1x, img/content/tablet/7-technology/4/1-2x.jpg 2x" media="(max-width: 1023px)")
                        source(type="image/webp" srcset="img/content/desktop/7-technology/4/1-1x.webp 1x, img/content/desktop/7-technology/4/1-2x.webp 2x") 
                        source(type="image/jpeg" srcset="img/content/desktop/7-technology/4/1-1x.jpg 1x, img/content/desktop/7-technology/4/1-2x.jpg 2x")
                        img(src="img/content/desktop/7-technology/4/1-1x.jpg")
                    .b-technology__post.b-technology__post--quality
                        div.b-title.b-title--quality Высокое качество
                        span.b-technology__info Все наши двери проходят строжайший контроль качества на&nbsp;всех этапах производства. Следить за&nbsp;качеством, мы&nbsp;считаем, важнейшая задача производителя.
