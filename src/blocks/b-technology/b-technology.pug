                    picture.b-picture.b-picture--vase
                        source(type="image/webp" srcset="img/content/mobile/7-technology/2/1-1x.webp 1x, img/content/mobile/7-technology/2/1-2x.webp 2x" media="(max-width: 767px)") 
                        source(type="image/jpeg" srcset="img/content/mobile/7-technology/2/1-1x.jpg 1x, img/content/mobile/7-technology/2/1-2x.jpg 2x" media="(max-width: 767px)")
                        source(type="image/webp" srcset="img/content/tablet/7-technology/2/1-1x.webp 1x, img/content/tablet/7-technology/2/1-2x.webp 2x" media="(max-width: 1023px)") 
                        source(type="image/jpeg" srcset="img/content/tablet/7-technology/2/1-1x.jpg 1x, img/content/tablet/7-technology/2/1-2x.jpg 2x" media="(max-width: 1023px)")
